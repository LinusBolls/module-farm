{
  "name": "minecraft-deviantart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-dev": "sh scripts/startDockerCompose.sh",
    "stop-dev": "docker compose stop",
    "clear-database": "npm run stop-dev && npm run start-dev",
    "restart-dev": "docker compose up -d --build",
    "build-prisma": "prisma generate && docker compose up -d --build && docker exec modulefarm npx prisma db push",
